#include<iostream>
#include<map>
#include<string>

using namespace std;

int main()
{
    int n;
    cin >> n;
    cin.ignore();
    
    map<string, int> freq;
    string line;
    
    for(int i=0; i<n; i++)
    {
        getline(cin, line);
        
        //extract the first word which is the country's name
        string country ="";
        for(int j=0; j<line.size(); j++)
        {
            if(line[j] ==' ')break;
            else
            country+=line[j];
        }
        freq[country]++;
    }
    
    for(auto &p: freq)
    {
        cout<< p.first <<" " << p.second << endl;
    }
    return 0;
}
