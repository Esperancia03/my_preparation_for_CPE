#include<iostream>
#include<vector>
#include<algorithm>
#include<cmath>

using namespace std;

int main()
{
// we should read first the number of test cases
int Test_cases, relatives, s[500];
cin >>Test_cases; //since the first line should contain the number of test cases

for(int i=0; i< Test_cases; i++)
{
cin >> relatives;

for(int j=0; j<relatives; j++)
{
cin >> s[j];
}
sort(s, s+relatives);
int median = s[relatives / 2];

        // Compute sum of distances
        int total = 0;
        for(int j = 0; j < relatives; j++) {
            total += abs(s[j] - median);
        }

        cout << total << endl;
    }

    return 0;
}
