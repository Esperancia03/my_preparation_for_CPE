#include<iostream>
#include<algorithm>
#include<string>
using namespace std;

class Reverse{
public:
  void digit_num(string num )
{

    reverse(num.begin(), num.end());

      if(num[0] == '0')
{
     for(int i=1; i< num.size(); i++)
{
     if(num[i] != '0')
{
      swap(num[0], num[i]);
       break;
}
}
   
}

cout << num;
}
};



int main(){


Reverse rev;
string  num;
cin>> num;
rev.digit_num(num);
}
