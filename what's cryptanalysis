#include<iostream>
#include<map>
#include<string>
#include<algorithm>
#include<vector>

using namespace std;


int main()
{
    int n;
    cin >> n;
    cin.ignore();
    
    vector<int> freq(26, 0);
    string line;
    
    for(int i=0; i<n; i++)
    {
        getline(cin, line);
        
        for(int c: line){
            if(isalpha(c)){
                c=toupper(c);
                freq[c-'A']++; //for the position of the other characters in the ASCII
                
            }
        }
        
    }
    
    //store the results, the letter plus the freq
    
    vector<pair<char, int>>result;
    
    for(int i=0; i<26; i++)
    {
        if(freq[i] >0)
        {
            result.push_back({char('A'+i), freq[i]});
        }
    }
    sort(result.begin(), result.end(), [](const pair<char, int> &a, const pair<char, int> &b)
    {
    if(a.second !=b.second)
    return a.second>b.second;
    return a.first<b.first;
    });
    
    for(auto &p: result){
        cout<<p.first << " "<<p.second <<endl;
    }
    return 0;
}
