#include<iostream>

using namespace std;

// they asked to write a prog in order to read an integer, a floating point and a character them display on the screen

//declare an integer, a float, then a character

int main()
{
    int a;
    double b;
    char c;
    cin >>a >> b>> c;
    cout << a <<endl;
    cout<< b <<endl;
    cout<<c;
    
    return 0;
}
