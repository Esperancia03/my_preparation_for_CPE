#include<iostream>
#include<string>
using namespace std;

int main()
{
    string n;
    while(cin >> n)
    {
        if(n =="0") break;
        
        int odd_sum=0;
        int even_sum = 0;
        
        for(int i =0; i<n.size(); i++)
        {
            int digit = n[i]- '0';
            if(i %2 ==0)
            odd_sum+= digit;
            else
            even_sum+=digit;
        }
        int diff = odd_sum-even_sum;
        if(diff % 11 ==0)
          cout << n << " is a multiple of 11." << endl;
        else
            cout << n << " is not a multiple of 11." << endl;
    }
    return 0;
}
