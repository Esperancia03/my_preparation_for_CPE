#include<iostream>
#include<algorithm>
using namespace std;

int cycles(long long n)
{
int count =1;
while (n!=1)
{
if(n%2==1)
{
 n= 3*n + 1;
}
else
{
n  = n/2;
}
count++;
}
return count;
}

int main()
{
    int i, j;
   while(cin >> i >> j)
   {
       int start =i;
       int end =j;
       
       if(start> end)
       {
         swap(start, end);
       }
           
           int maxcycles=0;
           
           for(int n=start; n<=end; n++)
           {
            int c = cycles(n);
            if(c > maxcycles) maxcycles = c;
           }
           cout << i << " " << j << " " << maxcycles;
   }
    
    return 0;
}
