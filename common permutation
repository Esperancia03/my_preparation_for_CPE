#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main() {
    string a, b;

    while (getline(cin, a) && getline(cin, b)) {
        vector<int> freqA(26, 0), freqB(26, 0);

        for (char c : a) freqA[c - 'a']++;
        for (char c : b) freqB[c - 'a']++;

        // Print the common sorted characters
        for (int i = 0; i < 26; i++) {
            int minimum = min(freqA[i], freqB[i]);
            while (minimum--) {
                cout << char('a' + i);
            }
        }
        cout << endl; // each pair's result on one line
    }

    return 0;
}
